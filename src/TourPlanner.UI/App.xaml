<Application x:Class="TourPlanner.UI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="clr-namespace:TourPlanner.UI.ViewModels"
             xmlns:views="clr-namespace:TourPlanner.UI.Views">
    <Application.Resources>
        <DataTemplate DataType="{x:Type vm:TourListViewModel}">
            <views:TourListView />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vm:TourLogsViewModel}">
            <views:TourLogsView />
        </DataTemplate>
    </Application.Resources>
</Application>
